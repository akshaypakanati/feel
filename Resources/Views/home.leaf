<html>
<body>
<h3>feel</h3>
<form method="post" action="/saveFeel">
  <input type="radio" name="feel" value="1">Extremely Happy<br>
 <input type="radio" name="feel" value="2">Happy<br>
 <input type="radio" name="feel" value="3">Neutral<br>
 <input type="radio" name="feel" value="4">Sad<br>
 <input type="radio" name="feel" value="5">Extremely Sad<br>
<p><input type="submit" /></p>
</form>
#for(feel in feels) {
    #(feel.feelId) -- #(feel.count) <br>
}
</body>
</html>
